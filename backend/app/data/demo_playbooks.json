    "key_statutes": [
      "Consumer Credit Act 1974",
      "Late Payment of Commercial Debts (Interest) Act 1998",
      "Insolvency Act 1986",
      "Civil Procedure Rules",
      "Limitation Act 1980"
    ],
    "success_factors": [
      "Clear documentation of debt",
      "Timely collection efforts",
      "Proper legal notices",
      "Asset investigation",
      "Consumer Credit Act compliance for regulated agreements"
    ]
