      }
    },
    "escalation_paths": [
      {
        "step": 1,
        "action": "Demand letter",
        "timeline": "30 days",
        "description": "Formal demand for payment with deadline"
      },
      {
        "step": 2,
        "action": "Payment plan negotiation",
        "timeline": "60 days",
        "description": "Negotiate structured payment arrangement"
      },
      {
        "step": 3,
