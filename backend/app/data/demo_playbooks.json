  {
    "id": "personal-injury",
    "case_type": "Personal Injury",
    "name": "Personal Injury Playbook",
    "description": "Comprehensive playbook for personal injury claims including motor vehicle accidents, slip and fall, and premises liability",
    "version": "1.0",
    "last_updated": "2024-01-15",
    "rules": [
      {
        "id": "rule-301",
        "condition": "clear_liability_established",
        "action": "pursue_full_damages",
        "weight": 0.9,
        "description": "If liability is clearly established, pursue full compensatory damages",
        "legal_basis": "Negligence law, strict liability",
        "evidence_required": ["Accident reports", "Witness statements", "Expert testimony", "Physical evidence"]
      },
      {
        "id": "rule-302",
        "condition": "comparative_negligence_issue",
        "action": "assess_fault_allocation",
        "weight": 0.7,
        "description": "If comparative negligence is an issue, assess fault allocation impact on damages",
        "legal_basis": "Comparative negligence statutes",
        "evidence_required": ["Accident reconstruction", "Traffic laws", "Witness testimony", "Video evidence"]
      },
      {
        "id": "rule-303",
        "condition": "serious_permanent_injury",
        "action": "maximize_pain_suffering_damages",
        "weight": 0.8,
