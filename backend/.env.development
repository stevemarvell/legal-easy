# Claude API Configuration - Development Environment
# Copy this file to .env and replace with your actual values

# Required: Your Claude API key from https://console.anthropic.com/
# Example: CLAUDE_API_KEY=sk-ant-api03-abcd1234efgh5678ijkl9012mnop3456qrst7890uvwx1234yzab5678cdef9012ghij3456klmn7890pqrs1234tuvw5678xyza9012bcde3456fghi7890jklm
CLAUDE_API_KEY=your_claude_api_key_here

# Claude API endpoint (usually don't need to change this)
CLAUDE_API_URL=https://api.anthropic.com/v1/messages

# Claude model to use
# Options: claude-3-opus-20240229, claude-3-sonnet-20240229, claude-3-haiku-20240307
CLAUDE_MODEL=claude-3-sonnet-20240229

# Maximum tokens for Claude responses
CLAUDE_MAX_TOKENS=4000

# Rate limiting configuration
# Requests per minute (adjust based on your API plan)
CLAUDE_RATE_LIMIT_RPM=50

# Tokens per minute (adjust based on your API plan)
CLAUDE_RATE_LIMIT_TPM=40000

# Optional: Environment identifier
ENVIRONMENT=development

# Optional: Debug logging
DEBUG=true